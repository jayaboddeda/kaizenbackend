<body class=" ">
    <!-- START TOPBAR -->
    <%-include("partials/navbar.ejs") %>
    <!-- END TOPBAR -->
    <!-- START CONTAINER -->
    <div class="page-container row-fluid container-fluid">

        <!-- SIDEBAR - START -->
        <%-include("partials/sidebar.ejs") %>
        <!--  SIDEBAR - END -->
        <!-- START CONTENT -->
        <section id="main-content" class=" ">
            <div class="wrapper main-wrapper row">
                <div class="row py-3 px-2 justify-content-between">
                    <div class="col-md-6">
                        <span class="head-name text-dark">Doctors</span>
                    </div>
                    <div class="col-md-6 btn-div">
                      <a href="/adddoctor"><button type="submit" value="submit" class="btn link1 ">+ Add Doctor</button></a>
                    </div>
                </div>
                <div class="clearfix"></div>
                <!-- MAIN CONTENT AREA STARTS -->

                    <div class="col-xs-12 mb-3">
                        <section class="">
                          <% for(let i =0; i < totaldoctors.length; i++){%>
                            <% if(totaldoctors[i].visibility == true){ %>
                            <div class="col-lg-3 col-md-6 mb-3">
                             
                                <div class="card text-center alldoctorscard">
                                  <div class="card-body">
                                  <a href="/doctorprofile/<%= totaldoctors[i]._id%>">
                                    <div> 
                                      <img src="./uploads/<%= totaldoctors[i].imgsrc%>" alt="">
                                    </div>
                                  </a>
                                    <div class="icon dots"><i class="fa fa-ellipsis-v" aria-hidden="true"></i>
                                      <div class="icon-btns border flex-column">
                                        <p class="mb-0"><a href="/editprofile/<%= totaldoctors[i]._id %>?formtype=edit">Edit</a></p>
                                        <p class="mb-0"><a href="/deletedoctor?id=<%= totaldoctors[i]._id %>">Delete</a></p>
                                      </div>
                                    </div>
                                  <a href="/doctorprofile/<%= totaldoctors[i]._id%>">

                                    <h5 class="card-title doctorname">Dr. <%= totaldoctors[i].firstName %> <%= totaldoctors[i].lastName %></h5>
                                    <p class="card-text doctorqual">
                                      <%= totaldoctors[i].experience[0].jobposition %>
                                    </p>
                                  </a>
                                  </div>
                                </div>
                            
                            </div>
                            <% } }%>
                        </section>
                    </div>
                </div>
                <% if(totaldoctors.length > 10){ %>
                    <div class="col-xs-12">
                        <section class="py-3 text-center">
                            <button type="submit" value="submit" class="btn link1 m-0 w-25">Load more</button>
                        </section>
                    </div>
                    <% } %>
                <!-- MAIN CONTENT AREA ENDS -->
                </div>
            </div>
        </section>
        
        <!-- END CONTENT -->
    </div>
