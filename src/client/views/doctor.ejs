    <script>
        $(document).ready(function () {

            $(document).on("scroll", onScroll);

            // smooth scroll

            $('.about-tabs-list ul li a').on('click', function (event) {
                $(document).off("scroll");
                var $anchor = $(this);
                $('.about-tabs-list ul li a').each(function () {
                    $(this).parent().removeClass('active');
                })
                $anchor.parent().addClass('active');
                var htx = $('header').innerHeight();
                $('html, body').stop().animate({
                    scrollTop: $($anchor.attr('href')).offset().top + 2 - htx
                }, 1000, 'swing', function () {

                    $(document).on("scroll", onScroll);
                });

                event.preventDefault();
            });

            onScroll()
            // adding active link while page scrolling
            function onScroll(event) {

                var hty = $('header').innerHeight();
                var scrollPos = $(document).scrollTop() + hty;
                $(' .about-tabs-list ul li a').each(function () {
                    var currLink = $(this);
                    var refElement = $(currLink.attr("href"));
                    if (refElement.position().top <= scrollPos && refElement.position().top + refElement
                        .height() > scrollPos) {
                        $('.about-tabs-list ul li').removeClass("active");
                        currLink.parent().addClass("active");
                    } else {
                        currLink.parent().removeClass("active");
                    }
                });
            }
            //============ Gallery Light Box
            var html5lightbox_options = {
                closeonoverlay: false,
                showplaybutton: false
            };
        });
        //======== Date and time
        $('#dateTimeCalendar').datetimepicker();
    </script>
